{
  "compilerOptions": {
    "baseUrl": "./src", 
    "paths": {
      "@components/*": ["components/*"],
      "@redux/*": ["redux/*"],
      "@sass/*": ["css/sass/*"],
      "@api/*": ["api/*"],
      "@hooks/*": ["hooks/*"],
      "@constants/*": ["constants/*"],
      "@utils/*": ["utils/*"],

    },
    "rootDir": "src",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
     "jsx": "react"
  },
  "include": ["src/**/*"]
}